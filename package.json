{
  "name": "Slack-clone-backend",
  "version": "0.0.1",
  "type": "module",
  "target": "es2022",
  "module": "es2022",
  "author": "Illia Chaban | Mykhailo Sichkaruk",
  "license": "MIT",
  "scripts": {
    "db-reset": "npx prisma db push --skip-generate --force-reset --accept-data-loss --schema=./src/infrastructure/prisma/schema.prisma",
    "db-migrate": "npx prisma migrate deploy --schema=./src/infrastructure/prisma/schema.prisma",
    "db-dev": "npx prisma db push --skip-generate --schema=./src/infrastructure/prisma/schema.prisma",
    "db-dev-migrate": "npx prisma migrate dev --skip-generate --schema=./src/infrastructure/prisma/schema.prisma",
    "compile": "tsc",
    "db-generate": "npx prisma generate --schema=./src/infrastructure/prisma/schema.prisma",
    "db-format": "npx prisma format --schema=./src/infrastructure/prisma/schema.prisma",
    "check-dev": "npm run db-format && npm run db-generate && npm run generate-grpc && npm run compile",
    "run-app": "node --experimental-json-modules ./src/app.js",
    "generate-grpc": "grpc_tools_node_protoc --ts_proto_out=./src/infrastructure --ts_proto_opt==outputServices=default,importSuffix=.js,outputServices=nice-grpc,oneof=unions,outputServices=generic-definitions,env=node,useReadonlyTypes=true,esModuleInterop=true --proto_path=./src/infrastructure/protos ./src/infrastructure/protos/authMicroServiceGRPC.proto",
    "lint": "eslint ./src",
    "development": "npm run db-migrate && npm run run-app",
    "production": "npm run db-migrate && npm run run-app"
  },
  "imports": {
    "#*": "./src/*",
    "#repo/*": "./src/domain/repo/*",
    "#model/*": "./src/domain/model/*",
    "#service/*": "./src/domain/service/*",
    "#impl/repo/*": "./src/application/impl/repo/*",
    "#impl/service/*": "./src/application/impl/service/*"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/oauth2": "^7.5.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.11.0",
    "@fastify/swagger-ui": "^1.10.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@fastify/under-pressure": "^8.3.0",
    "@grpc/grpc-js": "^1.9.5",
    "@grpc/proto-loader": "^0.7.9",
    "@prisma/client": "^5.4.2",
    "@sinclair/typebox": "^0.31.15",
    "@types/nodemailer": "^6.4.11",
    "fastify": "^4.23.2",
    "fastify-user-agent": "^1.0.1",
    "grpc-tools": "^1.12.4",
    "http-status-codes": "^2.2.0",
    "nice-grpc": "^2.1.0",
    "pino": "^8.15.6",
    "pino-pretty": "^10.2.3",
    "protobufjs": "^7.2.5",
    "socket.io": "^4.7.2",
    "ts-dotenv": "^0.9.1",
    "ts-opaque": "^3.0.1",
    "ts-proto": "^1.160.0",
    "ts-results-es": "^4.0.0",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/useragent": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-metarhia": "^8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "ts-json-schema-generator": "^1.2.0",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  }
}
